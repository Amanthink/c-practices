++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

class Solution {
public:
    int search(vector<int>& nums, int target) {
        int p;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==target){
                p=i;
                break;
            }
            else{
                p=-1;
            }
            
        }
        return p;
    }
};
